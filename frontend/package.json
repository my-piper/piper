    "@angular/router": "^17.3.2",
    "@apidevtools/json-schema-ref-parser": "^11.7.3",
    "@json-editor/json-editor": "^2.15.2",
    "ajv": "^8.17.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
